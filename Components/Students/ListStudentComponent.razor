@using Entities
@using Business

<table class="table table-hover table-bordered">
	<thead>
		<tr>
			<th>Id</th>
			<th>Nombre</th>
			<th>Apellido</th>
			<th>Programa</th>
			<th>Facultad</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var student in Students)
		{
			<tr>
				<td>@student.StudentId</td>
				<td>@student.StudentNombre</td>
				<td>@student.StudentApellido</td>
				<td>@student.StudentPrograma</td>
				<td>@student.StudentFacultad</td>
			</tr>
		}
	</tbody>
</table>
@code {
	List<Students> Students = new List<Students>();

	protected override async Task OnInitializedAsync()
	{
		Students = B_Student.students();
	}
}
